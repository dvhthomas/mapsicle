{
  "name": "interactive-travel-map",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "prebuild": "npm run test:run && npm run check",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./jsconfig.json --fail-on-warnings",
    "check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
    "geocode": "node scripts/geocode.js",
    "new-location": "node scripts/new-location.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/ui": "^4.0.5",
    "happy-dom": "^20.0.10",
    "svelte": "^4.2.0",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
    "vite": "^5.4.0",
    "vitest": "^4.0.5"
  },
  "dependencies": {
    "@maptiler/leaflet-maptilersdk": "^4.1.1",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.0",
    "leaflet": "^1.9.4",
    "mark.js": "^8.11.1",
    "marked": "^15.0.0"
  }
}
